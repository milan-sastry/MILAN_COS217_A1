NORMAL (accept)
    /: SLASH
    ": DOUBLE_QUOTE
    ': SINGLE_QUOTE
    other: NORMAL
SLASH (accept)
    /: SLASH
    *: START_STAR
    ": DOUBLE_QUOTE
    ': SINGLE_QUOTE
    other: NORMAL
IN_COMMENT (reject)
    *: END_STAR
    other: START_STAR
END_STAR (reject)
    /: NORMAL
    *: END_STAR
    other: IN_COMMENT
DOUBLE_QUOTE (accept)
    ": NORMAL
    \: DQ_BACKSLASH
    other: DOUBLE_QUOTE
SINGLE_QUOTE (accept)
    ': NORMAL
    \: SQ_BACKSLASH
    other: SINGLE_QUOTE
DQ_BACKSLASH (accept)
    any: DOUBLE_QUOTE
SQ_BACKSLASH (accept)
    any: SINGLE_QUOTE


